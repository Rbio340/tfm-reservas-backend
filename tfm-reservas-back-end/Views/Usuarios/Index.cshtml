@model IEnumerable<Reserva.Core.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsuNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuPassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Est)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsuNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuPassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Est.EstNombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UsuId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UsuId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UsuId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
